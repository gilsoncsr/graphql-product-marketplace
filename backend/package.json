{
  "name": "marketplace-graphql-bff",
  "version": "1.0.0",
  "description": "GraphQL BFF for Marketplace with Oracle DB",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon -r tsconfig-paths/register src/server.ts",
    "start": "node dist/server.js",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "db:seed": "node dist/scripts/seed.js"
  },
  "keywords": [
    "graphql",
    "typescript",
    "oracle",
    "marketplace",
    "bff"
  ],
  "author": "Marketplace Team",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^3.12.1",
    "bcryptjs": "^2.4.3",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dataloader": "^2.2.2",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "graphql": "^16.8.1",
    "graphql-depth-limit": "^1.1.0",
    "graphql-validation-complexity": "^0.4.0",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "oracledb": "^6.0.3",
    "pino": "^8.16.2",
    "pino-pretty": "^10.2.3",
    "redis": "^4.6.10",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cookie-parser": "^1.4.6",
    "@types/express": "^4.17.21",
    "@types/graphql-depth-limit": "^1.1.6",
    "@types/graphql-validation-complexity": "^0.4.4",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.8.10",
    "@types/oracledb": "^6.9.1",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.9.1",
    "@typescript-eslint/parser": "^6.9.1",
    "eslint": "^8.52.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lint-staged": "^15.0.2",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
